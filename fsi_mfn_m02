     SET eso_idx = eso_x
    ENDIF
   ENDFOR
  ENDIF
  IF ((eso_idx > 0 ) )
   RETURN (context->cerner.stringlist[eso_idx ].strval )
  ENDIF
  RETURN (" " )
 END ;Subroutine
 SUBROUTINE  set_esoinfo_string (sea_name ,svalue )
