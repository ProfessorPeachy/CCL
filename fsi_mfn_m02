   SET eso_idx = (list_size + 1 )
   SET stat = alterlist (context->cerner.stringlist ,eso_idx )
   SET context->cerner.stringlist[eso_idx ].strmeaning = cnvtlower (sea_name )
   SET context->cerner.stringlist[eso_idx ].strval = svalue
   RETURN (1 )
  ENDIF
 END ;Subroutine
