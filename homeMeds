;    Your Code Goes Here
SELECT DISTINCT INTO $OUTDEV
	ORD.ORDER_ID
	, ORD.ACTIVE_IND
	, ORD.ORIG_ORD_AS_FLAG
	, ORD.ORDER_MNEMONIC
	, P.ABS_BIRTH_DT_TM
	, P.NAME_FULL_FORMATTED
	, MRN = EA.ALIAS
	, FIN = EA2.ALIAS
	, ORD.TEMPLATE_ORDER_FLAG
	, OE.OE_FORMAT_NAME
	, OEF.DESCRIPTION
 
FROM
	ORDERS   ORD
	, PERSON   P
	, ENCOUNTER   E
	, ENCNTR_ALIAS   EA
	, ORDER_RECON   O
	, ORDER_DETAIL   OD
	, ENCNTR_ALIAS   EA2
	, ORDER_COMPLIANCE   OC
	, ORDER_COMPLIANCE_DETAIL   OCD
	, ORDER_ENTRY_FORMAT   OE
	, ORDER_ENTRY_FIELDS   OEF
 
PLAN P
JOIN E WHERE E.PERSON_ID = OUTERJOIN(P.PERSON_ID)
;and E.ENCNTR_STATUS_CD = ACT_VAR
;AND E.ENCNTR_TYPE_CD = INP_VAR
 
JOIN EA WHERE EA.ENCNTR_ID = OUTERJOIN(E.ENCNTR_ID)
AND EA.ENCNTR_ALIAS_TYPE_CD = MRN_VAR
AND EA.ALIAS = "1483088"
 
JOIN EA2 WHERE EA2.ENCNTR_ID = OUTERJOIN(E.ENCNTR_ID)
AND EA2.ENCNTR_ALIAS_TYPE_CD = FIN_VAR
;AND EA2.ALIAS = "200775092"
 
 
 
JOIN ORD WHERE ORD.ENCNTR_ID = OUTERJOIN(E.ENCNTR_ID)
AND ORD.ORIG_ORD_AS_FLAG = 2.00
AND ORD.CATALOG_TYPE_CD = 2516.00  /*CATALOG TYPE = PHARMACY*/
AND ORD.ORDER_STATUS_CD = 2550.00  /* ORDER STATUS = ORDERED*/
;and ORD.TEMPLATE_ORDER_FLAG = 0
 
JOIN O WHERE O.PERSON_ID = OUTERJOIN(P.PERSON_ID)
;AND O.RECON_STATUS_CD = 23290062.00   /*ORDER RECON STATUS = PARTIAL*/
;AND O.RECON_STATUS_CD = 23290060.00  /*ORDER RECON STATUS = COMPLETE*/
;AND O.RECON_TYPE_FLAG = 1
 
JOIN OD WHERE OD.ORDER_ID = OUTERJOIN(ORD.ORDER_ID)
;AND OD.OE_FIELD_MEANING = "RXROUTE"
 
JOIN OC WHERE OC.ENCNTR_ID = OUTERJOIN(E.ENCNTR_ID)
 
JOIN OCD WHERE OCD.ORDER_COMPLIANCE_ID = OUTERJOIN(OC.ORDER_COMPLIANCE_ID)
 
JOIN OE WHERE OE.OE_FORMAT_ID = ORD.OE_FORMAT_ID
 
JOIN OEF WHERE OEF.OE_FIELD_MEANING_ID = OD.OE_FIELD_MEANING_ID
and OEF.DESCRIPTION = "Drug Form"
 
ORDER BY
	ORD.ORDER_MNEMONIC
 
WITH NOCOUNTER, SEPARATOR=" ", FORMAT
